#
# Oracle installation configuration
#
ora_version: "19.3.0.0.0"
ora_release: "latest"
ora_edition: "EE"
cluster_type: "NONE" # NONE|RAC|DG
ora_swlib_bucket: "gs://bmaas-testing-oracle-software"
ora_swlib_type: "GCS"
ora_swlib_path: "/u01/swlib"
ora_staging: "/u01/swlib"
ora_disk_mgmt: "FS" # ASMLIB|ASMUDEV|UDEV|FS
ora_role_separation: "TRUE"
ora_data_destination: "/u02/oradata"
ora_reco_destination: "/u03/fast_recovery_area"
ora_asm_disks: ''
ora_asm_disks_json: ''
ora_data_mounts_json: '[{"blk_device":"/dev/disk/by-id/google-oracle_home","fstype":"xfs","mount_opts":"nofail","mount_point":"/u01","name":"u01","purpose":"software"},{"blk_device":"/dev/disk/by-id/google-data","fstype":"xfs","mount_opts":"nofail","mount_point":"/u02","name":"u02","purpose":"data"},{"blk_device":"/dev/disk/by-id/google-reco","fstype":"xfs","mount_opts":"nofail","mount_point":"/u03","name":"u03","purpose":"reco"}]'
swap_blk_device: "/dev/disk/by-id/google-swap"

#
# Database configuration
#
ora_db_name: "ORCL"
ora_db_domain: ""
ora_db_charset: "AL32UTF8"
ora_db_ncharset: "AL16UTF16"
ora_db_container: "False"
ora_db_type: "MULTIPURPOSE" # MULTIPURPOSE|DATA_WAREHOUSING|OLTP
ora_pdb_name_prefix: "PDB"
ora_pdb_count: 1
ora_redo_log_size: "100MB"
ora_pga_target_mb: 0
ora_sga_target_mb: 0
db_password_secret: "projects/abasinov-test/secrets/sys_password/versions/latest"

#
# Listener configuration
#
ora_listener_name: "LISTENER"
ora_listener_port: 1521

#
# Host configuration
#
instance_ip_addr: "10.128.0.35"
instance_hostname: "oracle-db"
instance_ssh_user: "sa_105448896290216900040"
instance_ssh_key: "/root/.ssh/google_compute_engine"
instance_ssh_extra_args: "-o ServerAliveInterval=60 -o ServerAliveCountMax=3 -o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null -o IdentityAgent=no"
ntp_pref: ""

#
# Data Guard configuration
