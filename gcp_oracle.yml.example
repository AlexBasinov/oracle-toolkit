#
# Backup configuration
#
backup_dest: "/u03/backups"
backup_redundancy: 2
archive_redundancy: 2
archive_online_days: 7
backup_level0_days: "0"
backup_level1_days: "1-6"
backup_start_hour: "01"
backup_start_min: "00"
archive_backup_min: "30"
backup_script_location: "/home/oracle/scripts"
backup_log_location: "/home/oracle/logs"
gcs_backup_config: ""
gcs_backup_bucket: ""
gcs_backup_temp_path: "/u01/gcsfusetmp"
nfs_backup_config: ""
nfs_backup_mount: ""

#
# Workload agent configuration
#
install_workload_agent: false
oracle_metrics_secret: "projects/abasinov-test/secrets/workload-agent-user-password/versions/latest"

#
# Other
#
skip_platform_compatibility: 0
compatible_rdbms: "0"
instance_hostgroup_name: "dbasm"
